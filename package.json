    "@types/node": "^12.11.1",
    "husky": "^6.0.0",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "tailwindcss": "2.1.4",
    "typescript": "~4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": []
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "tabWidth": 2,
